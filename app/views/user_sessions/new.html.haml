%h2 Login

%ol
  - form_for @user_session do |f|
    %li= f.error_messages
    %li
      = f.label :email
      = f.text_field :email

    %li
      = f.label :password
      = f.password_field :password

    %li
      = f.check_box :remember_me
      = f.label :remember_me

    %li
      = f.submit "Login"

